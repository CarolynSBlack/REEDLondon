               <xs:attribute name="place">
                  <xs:annotation>
                     <xs:documentation>specifies where this item is placed.</xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                     <xs:restriction>
                        <xs:simpleType>
                           <xs:list>
                              <xs:simpleType>
                                 <xs:restriction base="xs:token">
                                    <xs:enumeration value="bottom">
                                       <xs:annotation>
                                          <xs:documentation>The note to this REED Record is provided at the foot of the page</xs:documentation>
                                       </xs:annotation>
                                    </xs:enumeration>
                                    <xs:enumeration value="margin_left">
                                       <xs:annotation>
                                          <xs:documentation>The note to this REED Record is provided in the left margin</xs:documentation>
                                       </xs:annotation>
                                    </xs:enumeration>
                                    <xs:enumeration value="margin_right">
                                       <xs:annotation>
                                          <xs:documentation>The note to this REED Record is provided in the right margin</xs:documentation>
                                       </xs:annotation>
                                    </xs:enumeration>
                                    <xs:enumeration value="top">
                                       <xs:annotation>
                                          <xs:documentation>The note to this REED Record is provided at the top of the page</xs:documentation>
                                       </xs:annotation>
                                    </xs:enumeration>
                                    <xs:enumeration value="opposite">
                                       <xs:annotation>
                                          <xs:documentation>The note to this REED Record is provided on the opposite, i.e. facing, page</xs:documentation>
                                       </xs:annotation>
                                    </xs:enumeration>
                                    <xs:enumeration value="overleaf">
                                       <xs:annotation>
                                          <xs:documentation>The note to this REED Record is provided on the other side of the leaf</xs:documentation>
                                       </xs:annotation>
                                    </xs:enumeration>
                                    <xs:enumeration value="end">
                                       <xs:annotation>
                                          <xs:documentation>The note to this REED Record is provided at the end of e.g. chapter or volume.</xs:documentation>
                                       </xs:annotation>
                                    </xs:enumeration>
                                    <xs:enumeration value="inline">
                                       <xs:annotation>
                                          <xs:documentation>The note to this REED Record is provided within the body of the text.</xs:documentation>
                                       </xs:annotation>
                                    </xs:enumeration>
                                    <xs:enumeration value="inspace">
                                       <xs:annotation>
                                          <xs:documentation>The note to this REED Record is provided in a predefined space, for example left by an earlier scribe.</xs:documentation>
                                       </xs:annotation>
                                    </xs:enumeration>
                                    <xs:enumeration value="over">
                                       <xs:annotation>
                                          <xs:documentation>The note to this REED Record is provided written over existing text</xs:documentation>
                                       </xs:annotation>
                                    </xs:enumeration>
                                 </xs:restriction>
                              </xs:simpleType>
                           </xs:list>
                        </xs:simpleType>
                        <xs:minLength value="1"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute name="type">
                  <xs:annotation>
                     <xs:documentation>characterizes the element in some sense, using any convenient classification scheme or typology.</xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                     <xs:restriction base="xs:token">
                        <xs:enumeration value="antiquarian_source">
                           <xs:annotation>
                              <xs:documentation>The note to this REED Record is from antiquarian source</xs:documentation>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="edDesc">
                           <xs:annotation>
                              <xs:documentation>This note is the editorial note on a print source from which record/s were derived</xs:documentation>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="foot">
                           <xs:annotation>
                              <xs:documentation>The note to this REED Record is at the foot of the leaf</xs:documentation>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="marginal">
                           <xs:annotation>
                              <xs:documentation>The note to this REED Record is in the margin</xs:documentation>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="subhead">
                           <xs:annotation>
                              <xs:documentation>The note to this REED Record is a subheading</xs:documentation>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="techDesc">
                           <xs:annotation>
                              <xs:documentation>This note is the full bibliographic citation for a print source from which record/s were derived</xs:documentation>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="editorial">
                           <xs:annotation>
                              <xs:documentation>The note to this REED Record is an editorial note</xs:documentation>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="other">
                           <xs:annotation>
                              <xs:documentation>The note to this REED Record is another type of note. (If you use this more than a handful of times we should update the TEI ODD Customisation.)</xs:documentation>
                           </xs:annotation>
                        </xs:enumeration>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute name="anchored" default="true" type="xs:boolean">
                  <xs:annotation>
                     <xs:documentation>indicates whether the copy text shows the exact place of reference for the note.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="targetEnd">
                  <xs:annotation>
                     <xs:documentation>points to the end of the span to which the note is attached, if the note is not embedded in the text at that point.</xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                     <xs:restriction>
                        <xs:simpleType>
                           <xs:list itemType="xs:anyURI"/>
                        </xs:simpleType>
                        <xs:minLength value="1"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
